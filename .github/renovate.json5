{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  extends: [
        "config:base",
        ":dependencyDashboard",
        ":rebaseStalePrs",
        ":prConcurrentLimit10",
        "schedule:daily",
    ],
  onboarding: true,
  labels: [
        "dependencies",
        "renovate"
    ],
  enabledManagers: [
        "cargo",
        "github-actions"
    ],
  regexManagers: [
        {
      fileMatch: [
                "^Cargo\\.toml$"
            ],
      matchStrings: [
                "rust-version\\s*=\\s*\"(?<currentValue>.*?)\""
            ],
      depNameTemplate: "rust", // ðŸ‘ˆ same dep name
      datasourceTemplate: "rust-version"
        },
        {
      fileMatch: [
                "^rust-toolchain(?:\\.toml)?$"
            ],
      matchStrings: [
                "channel\\s*=\\s*\"(?<currentValue>.*?)\""
            ],
      depNameTemplate: "rust", // ðŸ‘ˆ same dep name = same version
      datasourceTemplate: "rust-version"
        }
    ],
  packageRules: [
        {
      matchManagers: [
                "cargo"
            ],
      groupName: "rust dependencies",
      groupSlug: "rust-deps",
      matchUpdateTypes: [
                "minor",
                "patch"
            ],
      automerge: false,
        },
        {
      matchManagers: [
                "cargo"
            ],
      matchUpdateTypes: [
                "major"
            ],
      automerge: false,
        },
    ],
  timezone: "UTC",
  dependencyDashboard: true,
}